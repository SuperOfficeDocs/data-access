{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "samples/**/*.*",
          "**/includes/**",
          "**/util/**",
          "***/contributing.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "src": "docs",
        "dest": "."
      }
    ],    
    "dest": "_site",
    "globalMetadata": {
      
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}