{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "*.{md,yml}" 
        ],
        "exclude": [
          "samples/**/*.*",
          "**/includes/**",
          "**/util/**",
          "***/contributing.md",
          "**/tmp/**",
          "assets/**/*.*"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ]
      }
    ],    
    "dest": "_site",
    "globalMetadata": {
      
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}